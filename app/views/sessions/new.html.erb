<h1>Sign In</h1>

  <form action="<%= session_url  %>" method="POST">
    <%= auth_token %>
    <label>Username or Email
      <input type="text" name="user[handle]">
    </label>
    <label>Password
      <input type="password" name="user[password]">
    </label>

    <button value="submit">Sign In</button>
  </form>
